#include "bits/stdc++.h"
using namespace std;
int main(){
	int n;
	cin>>n;
	vector<int>v(n);
	for(int i=0;i<n;i++) cin>>v[i];
	int mx=INT_MIN,mn=INT_MAX;
	for(int i=0;i<n;i++) {
		cin>>v[i];
		mx=max(mx,v[i]),mn=min(mn,v[i]);
	}
	cout<<"Max = "<<mx<<"\nMin = "<<mn<<"\n";
}

// link : https://www.geeksforgeeks.org/maximum-and-minimum-in-an-array/
